	<rdfs:Class rdf:about="Graph" rdfs:label="Graph" rdfs:comment="A Constellation graph." vs:term_status="stable">
		<rdf:type rdf:resource="owl:Class" />
		<rdfs:isDefinedBy rdf:resource="http://www.constellation-app.com/ns"/>
	</rdfs:Class>

	<rdfs:Class rdf:about="Node" rdfs:label="Node" rdfs:comment="A Constellation node." vs:term_status="stable">
		<rdf:type rdf:resource="owl:Class" />
		<rdfs:isDefinedBy rdf:resource="http://www.constellation-app.com/ns"/>
	</rdfs:Class>

	<rdfs:Class rdf:about="Transaction" rdfs:label="Transaction" rdfs:comment="A Constellation transaction." vs:term_status="stable">
		<rdf:type rdf:resource="owl:Class" />
		<rdfs:isDefinedBy rdf:resource="http://www.constellation-app.com/ns"/>
	</rdfs:Class>

	<rdfs:Class rdf:about="Attribute" rdfs:label="Attribute" rdfs:comment="A Constellation attribute." vs:term_status="stable">
		<rdf:type rdf:resource="owl:Class" />
		<rdf:type rdf:resource="owl:FunctionalProperty"/>
		<rdf:type rdf:resource="owl:DatatypeProperty"/>
		<rdfs:isDefinedBy rdf:resource="http://www.constellation-app.com/ns"/>
	</rdfs:Class>


<!--
	<rdf:Property rdf:about=":gender" vs:term_status="testing" rdfs:label="gender" rdfs:comment="The gender of this Agent (typically but not necessarily 'male' or 'female').">
		<rdf:type rdf:resource="owl:FunctionalProperty"/>
		<rdf:type rdf:resource="owl:DatatypeProperty"/>
		<rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
		<rdfs:range rdf:resource="rdfs:Literal"/>
		<rdfs:isDefinedBy rdf:resource="http://www.constellation-app.com/ns"/>
		&lt;!&ndash; whatever one's gender is, and we are liberal in leaving room for more options
