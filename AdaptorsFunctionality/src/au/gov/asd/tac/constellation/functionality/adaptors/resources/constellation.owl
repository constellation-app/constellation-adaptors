	<!-- Here we describe general characteristics of the Constellation vocabulary ('ontology'). -->
	<owl:Ontology rdf:about="" rdfs:label="Constellation vocabulary" rdfs:comment="The Constellation RDF vocabulary, described using W3C RDF Schema and the Web Ontology Language." >
		<owl:imports rdf:resource="http://www.w3.org/TR/2004/REC-owl-guide-20040210/food"/>
	</owl:Ontology>

